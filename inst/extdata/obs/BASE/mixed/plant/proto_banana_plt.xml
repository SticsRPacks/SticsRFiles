<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<fichierplt>
    <formalisme nom="plant name and group">
        <param format="character" nom="codeplante">ban</param>
        <option choix="2" nom="monocot or dicot" nomParam="codemonocot">
            <choix code="1" nom="monocotyledon"/>
            <choix code="2" nom="dicotyledon"/>
        </option>
    </formalisme>
    <formalisme nom="effect of atmospheric CO2 concentration">
        <param format="real" max="2.0" min="1.0" nom="alphaco2">1.20000</param>
    </formalisme>
    <formalisme nom="phasic development">
        <param format="real" max="15.0" min="-10.0" nom="tdmin">14.00000</param>
        <param format="real" max="40.0" min="15.0" nom="tdmax">35.00000</param>
        <option choix="2" nom="driving temperature" nomParam="codetemp">
            <choix code="1" nom="air temperature">
                <option choix="1" nom="time.scale" nomParam="codegdh">
                    <choix code="1" nom="daily.scale"/>
                    <choix code="2" nom="hourly.scale"/>
                </option>
            </choix>
            <choix code="2" nom="temperature within canopy">
                <param format="real" max="2.0" min="1.0" nom="coeflevamf">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coefamflax">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coeflaxsen">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coefsenlan">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coeflevdrp">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coefdrpmat">1.00000</param>
                <param format="real" max="2.0" min="1.0" nom="coefflodrp">1.00000</param>
            </choix>
        </option>
        <option choix="2" nom="photoperiodic plant" nomParam="codephot">
            <choix code="1" nom="yes">
                <param format="real" max="24.0" min="0.0" nom="phobase">0.000</param>
                <param format="real" max="24.0" min="0.0" nom="phosat">0.000</param>
            </choix>
            <choix code="2" nom="no"/>
        </option>
        <option choix="1" nom="delay effect of stress" nomParam="coderetflo">
            <choix code="1" nom="yes">
                <param format="real" max="0.9" min="0.1" nom="stressdev">0.20000</param>
            </choix>
            <choix code="2" nom="no"/>
        </option>
        <option choix="1" nom="cold requirements" nomParam="codebfroid">
            <choix code="1" nom="no"/>
            <choix code="2" nom="vernalisation (herbaceous)">
                <param format="integer" max="7" min="0" nom="jvcmini">7.00000</param>
                <param format="integer" max="731" min="1" nom="julvernal">274</param>
                <param format="real" max="10.0" min="-5.0" nom="tfroid">6.50000</param>
                <param format="real" max="30.0" min="1.0" nom="ampfroid">10.00000</param>
            </choix>
            <choix code="3" nom="dormancy (woody)">
                <param format="real" max="20000.0" min="0.0" nom="stdordebour">100.000</param>
                <param format="real" max="40.0" min="0.0" nom="tdmindeb">5.00000</param>
                <param format="real" max="40.0" min="0.0" nom="tdmaxdeb">25.00000</param>
                <option choix="3" nom="dormancy calculation" nomParam="codedormance">
                    <choix code="1" nom="forcing">
                        <param format="integer" max="731" min="1" nom="ifindorm">70</param>
                    </choix>
                    <choix code="2" nom="Richardson"/>
                    <choix code="3" nom="Bidabe">
                        <param format="real" max="3.5" min="1.5" nom="q10">3.00000</param>
                        <param format="integer" max="731" min="1" nom="idebdorm">300</param>
                    </choix>
                </option>
                <option choix="2" nom="post dormancy calculation" nomParam="codegdhdeb">
                    <choix code="1" nom="daily temperatures"/>
                    <choix code="2" nom="hourly temperatures"/>
                </option>
            </choix>
        </option>
    </formalisme>
    <formalisme nom="emergence and starting">
        <option choix="1" nom="annual or perennial" nomParam="codeperenne">
            <choix code="1" nom="annual">
                <option choix="2" nom="germination or latency" nomParam="codegermin">
                    <choix code="1" nom="yes">
                        <param format="real" max="15.0" min="-10.0" nom="tgmin">14.00000</param>
                        <param format="real" max="100.0" min="0.0" nom="stpltger">0.00000</param>
                        <param format="real" max="1.0" min="1.0E-4" nom="potgermi">-1.60000</param>
                        <param format="integer" max="50" min="1" nom="nbjgerlim">50</param>
                        <param format="real" max="100.0" min="0.0" nom="propjgermin">1.00000</param>
                    </choix>
                    <choix code="2" nom="no"/>
                </option>
                <option choix="2" nom="plantlet growth" nomParam="codehypo">
                    <choix code="1" nom="hypocotyle growth">
                        <param format="real" max="0.04" min="0.0050" nom="belong">999.00000</param>
                        <param format="real" max="10.0" min="1.0" nom="celong">999.00000</param>
                        <param format="real" max="40.0" min="2.0" nom="elmax">0.00000</param>
                        <param format="integer" max="100" min="1" nom="nlevlim1">10</param>
                        <param format="integer" max="100" min="1" nom="nlevlim2">50</param>
                        <param format="real" max="1.0" min="1.0E-4" nom="vigueurbat">1.00000</param>
                    </choix>
                    <choix code="2" nom="planting">
                        <param format="real" max="8.0" min="0.0" nom="laiplantule">0.10000</param>
                        <param format="integer" max="10" min="0" nom="nbfeuilplant">0</param>
                        <param format="real" max="4.0" min="0.0020" nom="masecplantule">0.50000</param>
                        <param format="real" max="200.0" min="0.0" nom="zracplantule">2.00000</param>
                    </choix>
                </option>
            </choix>
            <choix code="2" nom="perennial"/>
        </option>
    </formalisme>
    <formalisme nom="leaves">
        <param format="real" max="150.0" min="10.0" nom="phyllotherme">83.00000</param>
        <param format="real" max="200.0" min="1.0" nom="bdens">0.40000</param>
        <param format="real" max="1.0" min="0.0" nom="laicomp">0.30000</param>
        <param format="real" max="2.0" min="0.1" nom="hautbase">0.20000</param>
        <param format="real" max="5.0" min="0.1" nom="hautmax">2.50000</param>
        <param format="real" max="100.0" min="0.0" nom="tcxstop">100.00000</param>
        <option choix="1" nom="leaf dynamics" nomParam="codelaitr">
            <choix code="1" nom="LAI">
                <param format="real" max="2.5" min="1.5" nom="vlaimax">2.20000</param>
                <param format="real" max="10.0" min="0.0" nom="pentlaimax">5.50000</param>
                <param format="real" max="3.0" min="1.0" nom="udlaimax">3.00000</param>
                <param format="real" max="1.0" min="0.0" nom="ratiodurvieI">1.00000</param>
                <param format="real" max="15.0" min="-10.0" nom="tcmin">14.00000</param>
                <param format="real" max="50.0" min="10.0" nom="tcmax">40.00000</param>
                <param format="real" max="1.0" min="0.0" nom="ratiosen">0.00000</param>
                <param format="real" max="100.0" min="0.0" nom="abscission">0.00000</param>
                <param format="real" max="20.0" min="10.0" nom="parazofmorte">13.00000</param>
                <param format="real" max="1.0" min="-2.0" nom="innturgmin">0.30000</param>
                <param format="real" max="1.0" min="0.01" nom="dlaimin">0.00000</param>
                <option choix="1" nom="LAI calculation option" nomParam="codlainet">
                    <choix code="1" nom="direct LAInet">
                        <param format="real" max="0.5" min="5.0E-6" nom="dlaimax">0.02050</param>
                        <param format="real" max="1.0" min="0.3" nom="tustressmin">0.70000</param>
                    </choix>
                    <choix code="2" nom="LAInet=LAIbrut-senes">
                        <param format="real" max="0.5" min="5.0E-6" nom="dlaimaxbrut">0.02050</param>
                        <param format="real" max="1.0" min="0.0" nom="durviesupmax">0.00000</param>
                        <param format="real" max="1.0" min="-2.0" nom="innsen">0.99000</param>
                        <param format="real" max="1.5" min="0.5" nom="rapsenturg">0.50000</param>
                    </choix>
                </option>
                <option choix="2" nom="effect of photoperiod on senescence" nomParam="codestrphot">
                    <choix code="1" nom="oui">
                        <param format="real" max="12.0" min="1.0" nom="phobasesen">12.00000</param>
                        <param format="real" max="0.1" min="0.01" nom="dltamsmaxsen">0.01000</param>
                        <param format="real" max="0.1" min="0.01" nom="dltamsminsen">0.15000</param>
                        <param format="real" max="1000.0" min="0.0" nom="alphaphot">200.00000</param>
                    </choix>
                    <choix code="2" nom="non"/>
                </option>
            </choix>
            <choix code="2" nom="ground cover">
                <param format="real" max="1.0" min="0.5" nom="tauxrecouvmax">1.00000</param>
                <param format="real" max="1.0" min="0.5" nom="tauxrecouvkmax">1.00000</param>
                <param format="real" max="10.0" min="0.0" nom="pentrecouv">4.50000</param>
                <param format="real" max="3.0" min="0.0" nom="infrecouv">0.85000</param>
            </choix>
        </option>
    </formalisme>
    <formalisme nom="radiation interception">
        <option choix="1" nom="radiation interception" nomParam="codetransrad">
            <choix code="1" nom="Beer's law">
                <param format="real" max="1.5" min="0.1" nom="extin">0.65000</param>
            </choix>
            <choix code="2" nom="radiation transfers">
                <param format="real" max="2.0" min="0.1" nom="ktrou">0.00000</param>
                <param format="integer" max="3" min="1" nom="forme">0</param>
                <param format="real" max="5.0" min="-5.0" nom="rapforme">0.00000</param>
                <param format="real" max="10.0" min="-10.0" nom="adfol">0.00000</param>
                <param format="real" max="10.0" min="1.0" nom="dfolbas">0.00000</param>
                <param format="real" max="10.0" min="1.0" nom="dfolhaut">0.00000</param>
            </choix>
        </option>
    </formalisme>
    <formalisme nom="shoot biomass growth">
        <param format="real" max="15.0" min="-10.0" nom="temin">14.00000</param>
        <param format="real" max="40.0" min="15.0" nom="temax">40.00000</param>
        <param format="real" max="30.0" min="10.0" nom="teopt">21.00000</param>
        <param format="real" max="30.0" min="10.0" nom="teoptbis">21.00000</param>
        <param format="real" max="7.0" min="1.0" nom="efcroijuv">0.00000</param>
        <param format="real" max="10.0" min="1.0" nom="efcroiveg">1.75000</param>
        <param format="real" max="10.0" min="1.0" nom="efcroirepro">1.75000</param>
        <param format="real" max="0.5" min="0.0" nom="remobres">0.00000</param>
        <param format="real" max="50.0" min="2.0" nom="coefmshaut">0.00000</param>
    </formalisme>
    <formalisme nom="partitioning of biomass in organs">
        <param format="real" max="500.0" min="50.0" nom="slamax">110.000</param>
        <param format="real" max="500.0" min="50.0" nom="slamin">90.000</param>
        <param format="real" max="3.0" min="0.0" nom="tigefeuil">0.00000</param>
        <param format="real" max="0.5" min="0.0" nom="envfruit">0.00000</param>
        <param format="real" max="300.0" min="0.0" nom="sea">0.00000</param>
    </formalisme>
    <formalisme nom="yield formation">
        <option choix="1" nom="growing dynamics" nomParam="codeindetermin">
            <choix code="1" nom="determinate growing plant">
                <param format="integer" max="40" min="5" nom="nbjgrain">60</param>
                <param format="real" max="1.0" min="0.01" nom="cgrain">0.021</param>
                <param format="real" max="15000.0" min="0.0" nom="cgrainv0">0.600</param>
                <param format="real" max="10000.0" min="0.0" nom="nbgrmin">0.000</param>
                <option choix="1" nom="unit Harvest Index" nomParam="codeir">
                    <choix code="1" nom="days">
                        <param format="real" max="0.02" min="0.0010" nom="vitircarb">0.01800</param>
                        <param format="real" max="1.0" min="0.2" nom="irmax">0.50000</param>
                    </choix>
                    <choix code="2" nom="degree days">
                        <param format="real" max="0.0020" min="5.0E-5" nom="vitircarbT">0.00000</param>
                    </choix>
                </option>
            </choix>
            <choix code="2" nom="indeterminate growing plant">
                <param format="integer" max="20" min="1" nom="nboite">10</param>
                <param format="real" max="1.0" min="0.05" nom="allocfrmax">0.60000</param>
                <param format="real" max="1.0" min="0.01" nom="afpf">0.40000</param>
                <param format="real" max="30.0" min="0.0" nom="bfpf">4.32000</param>
                <param format="real" max="100.0" min="0.1" nom="cfpf">0.00000</param>
                <param format="real" max="5.0" min="0.01" nom="dfpf">0.20000</param>
                <param format="real" max="6000.0" min="0.0" nom="stdrpnou">0.00000</param>
                <param format="real" max="1.0" min="0.0" nom="spfrmin">0.00000</param>
                <param format="real" max="2.0" min="0.7" nom="spfrmax">1.00000</param>
                <param format="real" max="1.0" min="0.01" nom="splaimin">0.00000</param>
                <param format="real" max="2.0" min="0.7" nom="splaimax">1.00000</param>
                <option choix="1" nom="number of inflorescences" nomParam="codcalinflo">
                    <choix code="1" nom="prescribed">
                        <param format="real" max="100.0" min="0.0" nom="nbinflo">1.00000</param>
                    </choix>
                    <choix code="2" nom="trophic status function">
                        <param format="real" max="100.0" min="0.0" nom="inflomax">5.00000</param>
                        <param format="real" max="10.0" min="0.0" nom="pentinflores">0.80000</param>
                    </choix>
                </option>
            </choix>
        </option>
        <option choix="2" nom="thermal stress on filling" nomParam="codetremp">
            <choix code="1" nom="yes">
                <param format="real" max="20.0" min="0.0" nom="tminremp">0.00000</param>
                <param format="real" max="40.0" min="10.0" nom="tmaxremp">0.00000</param>
            </choix>
            <choix code="2" nom="no"/>
        </option>
        <param format="real" max="0.01" min="0.0010" nom="vitpropsucre">0.00075</param>
        <param format="real" max="0.01" min="0.0010" nom="vitprophuile">0.00000</param>
        <param format="real" max="0.04" min="0.0010" nom="vitirazo">0.01200</param>
    </formalisme>
    <formalisme nom="roots">
        <param format="real" max="1.0" min="0.0" nom="sensanox">1.00000</param>
        <param format="character" nom="stoprac">sen</param>
        <param format="real" max="1.0" min="0.0" nom="sensrsec">0.00000</param>
        <param format="real" max="1.0" min="0.0" nom="contrdamax">0.30000</param>
        <option choix="2" nom="driving temperature" nomParam="codetemprac">
            <choix code="1" nom="temperature within the canopy"/>
            <choix code="2" nom="soil (threshold TGMIN)"/>
        </option>
        <option choix="1" nom="root density" nomParam="coderacine">
            <choix code="1" nom="standard profile">
                <param format="real" max="100.0" min="0.0" nom="zlabour">27.00000</param>
                <param format="real" max="200.0" min="10.0" nom="zpente">95.00000</param>
                <param format="real" max="200.0" min="10.0" nom="zprlim">150.00000</param>
            </choix>
            <choix code="2" nom="true density">
                <param format="real" max="1000.0" min="1.0" nom="draclong">0.000</param>
                <param format="real" max="2000.0" min="0.0" nom="debsenrac">0.000</param>
                <param format="real" max="1.0" min="0.02" nom="lvfront">0.05000</param>
                <param format="real" max="25000.0" min="250.0" nom="longsperac">9091.000</param>
                <option choix="2" nom="N effect on root distribution" nomParam="codazorac">
                    <choix code="1" nom="yes">
                        <param format="real" max="1.0" min="0.0" nom="minefnra">0.00000</param>
                        <param format="real" max="5.0" min="0.0" nom="minazorac">0.10000</param>
                        <param format="real" max="5.0" min="0.1" nom="maxazorac">1.00000</param>
                    </choix>
                    <choix code="2" nom="no"/>
                </option>
                <option choix="3" nom="trophic-linked production" nomParam="codtrophrac">
                    <choix code="1" nom="continuous link">
                        <param format="real" max="0.9" min="0.1" nom="repracpermax">0.40000</param>
                        <param format="real" max="0.9" min="0.1" nom="repracpermin">0.20000</param>
                        <param format="real" max="1.0" min="1.0E-4" nom="krepracperm">1.27000</param>
                    </choix>
                    <choix code="2" nom="threshold">
                        <param format="real" max="0.9" min="0.1" nom="repracseumax">0.40000</param>
                        <param format="real" max="0.9" min="0.1" nom="repracseumin">0.20000</param>
                        <param format="real" max="1.0" min="1.0E-4" nom="krepracseu">1.27000</param>
                    </choix>
                    <choix code="3" nom="no"/>
                </option>
            </choix>
        </option>
    </formalisme>
    <formalisme nom="frost">
        <param format="real" max="-1.0" min="-30.0" nom="tletale">-1.00000</param>
        <param format="real" max="5.0" min="-5.0" nom="tdebgel">0.00000</param>
        <option choix="2" nom="plantlet or emergence frost" nomParam="codgellev">
            <choix code="1" nom="no"/>
            <choix code="2" nom="yes">
                <param format="integer" max="5" min="1" nom="nbfgellev">3</param>
                <param format="real" max="0.0" min="-25.0" nom="tgellev10">0.00000</param>
                <param format="real" max="0.0" min="-25.0" nom="tgellev90">-1.00000</param>
            </choix>
        </option>
        <option choix="2" nom="leaf frost at juvenile phase (till AMF)" nomParam="codgeljuv">
            <choix code="1" nom="no"/>
            <choix code="2" nom="yes">
                <param format="real" max="0.0" min="-25.0" nom="tgeljuv10">0.00000</param>
                <param format="real" max="0.0" min="-25.0" nom="tgeljuv90">-1.00000</param>
            </choix>
        </option>
        <option choix="2" nom="leaf frost at adult phase" nomParam="codgelveg">
            <choix code="1" nom="no"/>
            <choix code="2" nom="yes">
                <param format="real" max="0.0" min="-25.0" nom="tgelveg10">0.00000</param>
                <param format="real" max="0.0" min="-25.0" nom="tgelveg90">-1.00000</param>
            </choix>
        </option>
        <option choix="2" nom="flower/fruit frost (from FLO)" nomParam="codgelflo">
            <choix code="1" nom="no"/>
            <choix code="2" nom="yes">
                <param format="real" max="0.0" min="-25.0" nom="tgelflo10">0.00000</param>
                <param format="real" max="0.0" min="-25.0" nom="tgelflo90">-1.00000</param>
            </choix>
        </option>
    </formalisme>
    <formalisme nom="water">
        <param format="real" max="25.0" min="1.0" nom="psisto">5.00000</param>
        <param format="real" max="15.0" min="1.0" nom="psiturg">2.00000</param>
        <param format="real" max="1.0" min="0.5" nom="h2ofeuilverte">0.75000</param>
        <param format="real" max="1.0" min="0.05" nom="h2ofeuiljaune">0.25000</param>
        <param format="real" max="1.0" min="0.5" nom="h2otigestruc">0.85000</param>
        <param format="real" max="1.0" min="0.5" nom="h2oreserve">0.85000</param>
        <param format="real" max="1.0" min="0.1" nom="h2ofrvert">0.85000</param>
        <param format="real" max="0.02" min="-0.02" nom="deshydbase">0.00080</param>
        <param format="real" max="0.05" min="1.0E-4" nom="tempdeshyd">0.00000</param>
        <option choix="1" nom="water requirements" nomParam="codebeso">
            <choix code="1" nom="crop coefficient">
                <param format="real" max="4.0" min="0.5" nom="kmax">1.50000</param>
            </choix>
            <choix code="2" nom="resistance approach">
                <param format="real" max="500.0" min="20.0" nom="rsmin">0.00000</param>
            </choix>
        </option>
        <option choix="1" nom="interception of water by foliage" nomParam="codeintercept">
            <choix code="1" nom="yes">
                <param format="real" max="3.0" min="0.05" nom="mouillabil">1.00000</param>
                <param format="real" max="1.0" min="0.0" nom="stemflowmax">0.00000</param>
                <param format="real" max="2.0" min="0.1" nom="kstemflow">0.00000</param>
            </choix>
            <choix code="2" nom="no"/>
        </option>
    </formalisme>
    <formalisme nom="nitrogen">
        <param format="real" max="0.01" min="2.0E-4" nom="Vmax1">0.00180</param>
        <param format="real" max="200.0" min="20.0" nom="Kmabs1">50.000</param>
        <param format="real" max="0.1" min="0.0020" nom="Vmax2">0.01700</param>
        <param format="real" max="40000.0" min="4000.0" nom="Kmabs2">25000.000</param>
        <param format="real" max="7.0" min="1.0" nom="adil">3.50000</param>
        <param format="real" max="0.8" min="0.01" nom="bdil">0.37000</param>
        <param format="real" max="5.0" min="0.05" nom="masecNmax">1.00000</param>
        <param format="real" max="1.0" min="0.0" nom="INNmin">0.30000</param>
        <param format="real" max="1.0" min="0.0" nom="INNimin">-0.50000</param>
        <param format="real" max="2.0" min="0.3" nom="inngrain1">1.00000</param>
        <param format="real" max="2.0" min="0.3" nom="inngrain2">1.20000</param>
